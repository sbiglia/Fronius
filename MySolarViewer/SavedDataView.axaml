<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:mySolarViewer="clr-namespace:MySolarViewer"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MySolarViewer.SavedDataView">
	<Grid>
        <Grid RowDefinitions="0.5*,0.5*">
            <Grid.ColumnDefinitions>
                <ColumnDefinition MinWidth="200" Width="0.5*"></ColumnDefinition>
                <ColumnDefinition MinWidth="200" Width="0.5*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <mySolarViewer:TodayDataGraph Grid.Row="0" Grid.Column="0" />
            <mySolarViewer:WeeklyDataGraph Grid.Row="0" Grid.Column="1" />
            <mySolarViewer:MonthlyDataGraph Grid.Row="1" Grid.Column="0"/>
            <mySolarViewer:YearlyDataGraph Grid.Row="1" Grid.Column="1" />
        </Grid>
    </Grid>
</UserControl>
